<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/letssee">
<LinearLayout
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/mL"
    android:background="@drawable/guit">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtuser"
        android:layout_gravity="center"
        android:gravity="center"
        android:textSize="20dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Select a suitable date to book your slot/slots"
        android:layout_gravity="center"
        android:gravity="center"
        />

    <ListView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lvdates"
        android:layout_gravity="center"
        android:gravity="center">

    </ListView>

    <Button
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/BookedSlot"
        android:text="My Booked Slots"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="#000000"
        android:background="@drawable/drum"/>


    <Button
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/logout"
        android:text="logout"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:gravity="center"
        android:background="@drawable/drum"/>





 </LinearLayout>
</FrameLayout>
